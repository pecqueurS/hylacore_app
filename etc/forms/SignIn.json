{
	"Username": {
		"name": "login",
		"options": {
			"type": "text",
			"label": "Identifiant",
			"class": "input",
			"placeholder": "mon identifiant",
			"required": true,
			"value": true,
			"constraints":{
				"NotBlank": true,
				"Type": "string",
				"Regex": {
					"pattern": "/.*/",
					"match": true
				},
				"Length":{
					"min": 5,
					"max": 16
				}
			}
		}
	},
	"Password": {
		"name": "pwd",
		"options": {
			"type": "password",
			"label": "Mot de passe",
			"class": "input",
			"placeholder": "mon mot de passe",
			"required": true,
			"value": true,
			"constraints":{
				"NotBlank": true,
				"Type": "string",
				"Regex": {
					"pattern": "/.*/",
					"match": true
				},	
				"Length":{
					"min": 6,
					"max": 16
				}
			}
		}
	}
}
